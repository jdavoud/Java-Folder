							secondButton = 1;
					b14.setText(" ");
					whiteTurn = !whiteTurn;
					firstButton = true;
				}
			}
			if(b1 == "b16"){
				if(((JButton)event.getSource()).getText().equals(" ")&&((JButton)event.getSource()).getName().equals("b25")||((JButton)event.getSource()).getName().equals("b27")){
					((JButton)event.getSource()).setText(op);
					 
							secondButton = 1;

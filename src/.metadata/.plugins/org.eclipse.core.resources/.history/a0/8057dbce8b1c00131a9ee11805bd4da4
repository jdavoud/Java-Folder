	public static void title(){
		if(b12.getText().equals("е")){
			b12.setText("ее");
			blackGone -= 1;
		}
		if(b14.getText().equals("е")){
			b14.setText("ее");
			blackGone -= 1;
		}
		if(b16.getText().equals("е")){
			b16.setText("ее");
			blackGone -= 1;
		}
		if(b18.getText().equals("е")){
			b18.setText("ее");
			blackGone -= 1;
		}
		if(b81.getText().equals("o")){
			b81.setText("oo");
			whiteGone -= 1;
		}
		if(b83.getText().equals("o")){
			b83.setText("oo");
			whiteGone -= 1;
		}
		if(b85.getText().equals("o")){
			b85.setText("oo");
			whiteGone -= 1;
		}
		if(b87.getText().equals("o")){
			b87.setText("oo");
			whiteGone -= 1;
		}
		
		frame.setTitle(b1 + " " + op + " " + whiteTurn + " " + winner);
	}
	public static void checkWin(){
		if(!b12.getText().startsWith("е")&&!b14.getText().startsWith("е")&&!b16.getText().startsWith("е")&&!b18.getText().startsWith("е")
		&&!b21.getText().startsWith("е")&&!b23.getText().startsWith("е")&&!b25.getText().startsWith("е")&&!b27.getText().startsWith("е")
		&&!b32.getText().startsWith("е")&&!b34.getText().startsWith("е")&&!b36.getText().startsWith("е")&&!b38.getText().startsWith("е")
		&&!b41.getText().startsWith("е")&&!b43.getText().startsWith("е")&&!b45.getText().startsWith("е")&&!b47.getText().startsWith("е")
		&&!b52.getText().startsWith("е")&&!b54.getText().startsWith("е")&&!b56.getText().startsWith("е")&&!b58.getText().startsWith("е")
		&&!b61.getText().startsWith("е")&&!b63.getText().startsWith("е")&&!b65.getText().startsWith("е")&&!b67.getText().startsWith("е")
		&&!b72.getText().startsWith("е")&&!b74.getText().startsWith("е")&&!b76.getText().startsWith("е")&&!b78.getText().startsWith("е")
		&&!b81.getText().startsWith("е")&&!b83.getText().startsWith("е")&&!b85.getText().startsWith("е")&&!b87.getText().startsWith("е")){
			winner = "white";
			win();
		}
		if(!b12.getText().startsWith("o")&&!b14.getText().startsWith("o")&&!b16.getText().startsWith("o")&&!b18.getText().startsWith("o")
		&&!b21.getText().startsWith("o")&&!b23.getText().startsWith("o")&&!b25.getText().startsWith("o")&&!b27.getText().startsWith("o")
		&&!b32.getText().startsWith("o")&&!b34.getText().startsWith("o")&&!b36.getText().startsWith("o")&&!b38.getText().startsWith("o")
		&&!b41.getText().startsWith("o")&&!b43.getText().startsWith("o")&&!b45.getText().startsWith("o")&&!b47.getText().startsWith("o")
		&&!b52.getText().startsWith("o")&&!b54.getText().startsWith("o")&&!b56.getText().startsWith("o")&&!b58.getText().startsWith("o")
		&&!b61.getText().startsWith("o")&&!b63.getText().startsWith("o")&&!b65.getText().startsWith("o")&&!b67.getText().startsWith("o")
		&&!b72.getText().startsWith("o")&&!b74.getText().startsWith("o")&&!b76.getText().startsWith("o")&&!b78.getText().startsWith("o")
		&&!b81.getText().startsWith("o")&&!b83.getText().startsWith("o")&&!b85.getText().startsWith("o")&&!b87.getText().startsWith("o")){
			winner = "black";
			win();
		}
	
	}
	public static void win(){
		frame.setSize(400,700);
		if(winner == "white"){
			winLabel.setText("o wins");
		}
		if(winner == "black"){
			winLabel.setText("е wins");
		}
	}
	
}

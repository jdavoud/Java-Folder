	/* (non-Javadoc)
	 * @see java.awt.event.ActionListener#actionPerformed(java.awt.event.ActionEvent)
	 */
	@Override
	public void actionPerformed(ActionEvent event) {
		
		if (!((JButton)event.getSource()).getText().equals(" ")&& secondButton != 3){
			if(whiteTurn && (((JButton)event.getSource()).getText().equals("o")||((JButton)event.getSource()).getText().equals("oo"))){
				b1 = ((JButton)event.getSource()).getName();
				op = ((JButton)event.getSource()).getText();
				firstButton = false;	
				secondButton = 2;
			}
			if(!whiteTurn && (((JButton)event.getSource()).getText().equals("е")||((JButton)event.getSource()).getText().equals("ее"))){
				b1 = ((JButton)event.getSource()).getName();
				op = ((JButton)event.getSource()).getText();
				firstButton = false;	
				secondButton = 2;
			}
		}
		else if(((JButton)event.getSource()).getText().equals(" ") && secondButton == 2){
			if(b1 == "b12"){
				if(((JButton)event.getSource()).getText().equals(" ")&&((JButton)event.getSource()).getName().equals("b21")||((JButton)event.getSource()).getName().equals("b23")){
					
					((JButton)event.getSource()).setText(op);
					 
					secondButton = 1;
					whiteTurn = !whiteTurn;
					firstButton = true;
					b12.setText(" ");
				}
				if(((JButton)event.getSource()).getName().equals("b34")&&!b23.getText().equals(" ")&&!b23.getText().equals(op)&&!b23.getText().startsWith(op.substring(0, 1))){
					if(whiteTurn){
						if(b23.getText().equals("е")){
							blackGone += 1;
						}
						if(b23.getText().equals("ее")){
							blackGone += 2;
						}
					}
					else { 
						if(b23.getText().equals("o")){
							whiteGone += 1;
						}
						if(b23.getText().equals("oo")){
							whiteGone += 2;
						}
					}
					b23.setText(" ");
					b34.setText(op);
					 
					secondButton = 1;
					b12.setText(" ");
					whiteTurn = !whiteTurn;
					firstButton = true;
				}
			}
			if(b1 == "b14"){
				if(((JButton)event.getSource()).getText().equals(" ")&&((JButton)event.getSource()).getName().equals("b23")||((JButton)event.getSource()).getName().equals("b25")){
					((JButton)event.getSource()).setText(op);
					 
					secondButton = 1;
					whiteTurn = !whiteTurn;
					firstButton = true;
					b14.setText(" ");
				}
				if(((JButton)event.getSource()).getName().equals("b36")&&!b25.getText().equals(" ")&&!b25.getText().equals(op)&&!b25.getText().startsWith(op.substring(0, 1))){
					if(whiteTurn){
						if(b25.getText().equals("е")){
							blackGone += 1;
						}
						if(b25.getText().equals("ее")){
							blackGone += 2;
						}
					}
					else { 
						if(b25.getText().equals("o")){
							whiteGone += 1;
						}
						if(b25.getText().equals("oo")){
							whiteGone += 2;
						}
					}
					b25.setText(" ");
					b36.setText(op);
					 
					secondButton = 1;
					b14.setText(" ");
					whiteTurn = !whiteTurn;
					firstButton = true;
				}
				if(((JButton)event.getSource()).getName().equals("b32")&&!b23.getText().equals(" ")&&!b23.getText().equals(op)&&!b23.getText().startsWith(op.substring(0, 1))){
					if(whiteTurn){
						if(b23.getText().equals("е")){
							blackGone += 1;
						}
						if(b23.getText().equals("ее")){
							blackGone += 2;
						}
					}
					else { 
						if(b23.getText().equals("o")){

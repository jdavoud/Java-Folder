
public class mysteryFunc {
	public static int num = 12304;
	public static void main(String[] args) {
		// TODO Auto-generated method stub
		mystery(num);
	}
	public static void mystery(int x){
		System.out.println((x % 10) + Messages.getString("mysteryFunc.0") + x); //$NON-NLS-1$
		if((x / 10) != 0){
			mystery(x / 10);
		}
		System.out.println((x % 10) + Messages.getString("mysteryFunc.1") +  x + Messages.getString("mysteryFunc.2")); //$NON-NLS-1$ //$NON-NLS-2$
	}
}
